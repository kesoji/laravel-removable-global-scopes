{
    "name": "kesoji/laravel-removable-global-scopes",
    "description": "Dynamically remove global scopes from Eloquent models in Laravel",
    "keywords": [
        "laravel",
        "eloquent",
        "global-scopes",
        "orm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "kesoji",
            "email": "6465251+kesoji@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Kesoji\\RemovableGlobalScopes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kesoji\\RemovableGlobalScopes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {}
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
